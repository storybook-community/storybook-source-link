{
  "name": "@storybook-community/storybook-source-link",
  "version": "5.0.0",
  "description": "Provides a link to the story's source in the toolbar",
  "keywords": [
    "storybook-addons",
    "storybook-community",
    "storybook",
    "code",
    "source",
    "link"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storybook-community/storybook-source-link.git"
  },
  "type": "module",
  "author": "Jonathan Sirrine <sirrineprogramming@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build:storybook": "storybook build",
    "build:watch": "pnpm build --watch",
    "clean": "rimraf ./dist",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "pack": "pnpm pack --out storybook-source-link-$(date +%s).tgz",
    "prebuild": "pnpm clean",
    "prepare": "husky",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "pnpm build && pnpm semantic-release",
    "release:canary": "pnpm build && auto canary",
    "start": "run-p build:watch \"storybook --quiet\"",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.28.0",
    "@storybook/addon-docs": "next",
    "@storybook/addon-essentials": "next",
    "@storybook/addon-interactions": "next",
    "@storybook/addon-links": "next",
    "@storybook/blocks": "next",
    "@storybook/icons": "^1.4.0",
    "@storybook/react": "next",
    "@storybook/react-vite": "next",
    "@storybook/test": "next",
    "@storybook/theming": "next",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/semver": "^7.7.0",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-istanbul": "3.2.3",
    "auto": "^11.3.0",
    "boxen": "^8.0.1",
    "commitlint": "^19.8.1",
    "dedent": "^1.6.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.0",
    "npm-run-all2": "^8.0.1",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.5",
    "storybook": "next",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "zx": "^8.5.4"
  },
  "peerDependencies": {
    "storybook": "^9.0.0"
  },
  "resolutions": {
    "@octokit/core": "^7",
    "@octokit/request-error": "^7",
    "@octokit/request": "^10",
    "@octokit/plugin-paginate-rest": "^13"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.ts"
    ],
    "previewEntries": [
      "src/preview.ts"
    ]
  },
  "storybook": {
    "displayName": "Source Link (community version)",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://avatars.githubusercontent.com/u/216219414?s=200&v=4"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "@eslint/plugin-kit@<0.3.4": ">=0.3.4",
      "vite@>=6.0.0 <=6.3.5": ">=6.3.6",
      "vite@>=6.0.0 <=6.4.0": ">=6.4.1"
    }
  }
}
