{
  "name": "@storybook-community/storybook-source-link",
  "version": "5.0.0",
  "description": "Provides a link to the story's source in the toolbar",
  "keywords": [
    "storybook-addons",
    "storybook-community",
    "storybook",
    "code",
    "source",
    "link"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storybook-community/storybook-source-link.git"
  },
  "type": "module",
  "author": "Jonathan Sirrine <sirrineprogramming@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build:storybook": "storybook build",
    "build:watch": "pnpm build --watch",
    "clean": "rimraf ./dist",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "pack": "pnpm pack --out storybook-source-link-$(date +%s).tgz",
    "prebuild": "pnpm clean",
    "prepare": "husky",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "pnpm build && pnpm semantic-release",
    "release:canary": "pnpm build && auto canary",
    "start": "run-p build:watch \"storybook --quiet\"",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.37.0",
    "@storybook/addon-docs": "next",
    "@storybook/addon-essentials": "next",
    "@storybook/addon-interactions": "next",
    "@storybook/addon-links": "next",
    "@storybook/blocks": "next",
    "@storybook/icons": "^1.4.0",
    "@storybook/react": "next",
    "@storybook/react-vite": "next",
    "@storybook/test": "next",
    "@storybook/theming": "next",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/semver": "^7.7.1",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-istanbul": "3.2.4",
    "auto": "^11.3.0",
    "boxen": "^8.0.1",
    "commitlint": "^20.1.0",
    "dedent": "^1.7.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "npm-run-all2": "^8.0.1",
    "prettier": "^3.6.2",
    "prompts": "^2.4.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.9",
    "storybook": "next",
    "tsup": "^8.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vitest": "^3.2.4",
    "zx": "^8.8.4"
  },
  "peerDependencies": {
    "storybook": "^9.0.0"
  },
  "resolutions": {
    "@octokit/core": "^7",
    "@octokit/request-error": "^7",
    "@octokit/request": "^10",
    "@octokit/plugin-paginate-rest": "^13"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.ts"
    ],
    "previewEntries": [
      "src/preview.ts"
    ]
  },
  "storybook": {
    "displayName": "Source Link",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=22"
  }
}
